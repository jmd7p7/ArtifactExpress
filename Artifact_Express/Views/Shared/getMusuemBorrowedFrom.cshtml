@model Artifact_Express.TIMEPERIOD

@{
    ViewBag.Title = "getMuseum";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@using (Html.BeginForm("ArtifactsBorrowed", "Artifact"))
{
