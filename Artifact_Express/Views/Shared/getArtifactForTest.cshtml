@{
    ViewBag.Title = "getArtifactName";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@using (Html.BeginForm("GetAllToolsUsedInTest", "Conservation"))
{
    @Html.AntiForgeryToken()
    <h3>Enter artifact name to view tools used in its testing:</h3>
    <label>Artifact name: </label>@Html.TextBox("artifactName")
    <input type="submit" />
}
