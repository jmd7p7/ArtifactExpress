@model IEnumerable<Artifact_Express.COLLECTION>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Collections Index</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CollectionName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CollectionDescription)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.COUNTRY)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TIMEPERIOD)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CollectionName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CollectionDescription)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.COUNTRY.CountryName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TIMEPERIOD.TimePeriodName)
        </td>
    </tr>
}

</table>
