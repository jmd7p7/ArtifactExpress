@model IEnumerable<Artifact_Express.EXHIBIT>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ExhibitName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ExhibitDescription)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ExhibitBeginningDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ExhibitEndingDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.COLLECTION.CollectionName)
        </th>
        <th>
            Storage/Display
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ExhibitName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ExhibitDescription)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ExhibitBeginningDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ExhibitEndingDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.COLLECTION.CollectionName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LOCATION.StorageOrDisplay)
        </td>
    </tr>
}

</table>
