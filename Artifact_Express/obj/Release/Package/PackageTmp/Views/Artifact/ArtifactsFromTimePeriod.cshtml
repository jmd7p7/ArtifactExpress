@model IEnumerable<Artifact_Express.ArtifactsFromTimePeriod_Result>

@{
    ViewBag.Title = "ArtifactsFromTimePeriod";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Query Results</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ArtifactName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TimePeriodName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ArtifactName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TimePeriodName)
        </td>
    </tr>
}

</table>
