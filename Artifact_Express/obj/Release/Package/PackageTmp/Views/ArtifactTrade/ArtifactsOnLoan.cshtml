@model IEnumerable<Artifact_Express.ArtifactsOnLoan_Result>

@{
    ViewBag.Title = "ArtifactsOnLoan";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Query Results</h2>

<table class="table">
    <tr>
        <th>
            Museum
        </th>
        <th>
            Artifact
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.MuseumName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ArtifactName)
            </td>
        </tr>
    }

</table>